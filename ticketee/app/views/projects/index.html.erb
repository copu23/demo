
<header>
<h1>Projects</h1>
<ul class="actions">

<li><%= link_to fa_icon("plus-square ") + " New Project", new_project_path,
class: "btn btn-success"%></li>
</ul>
</div>
<div id="Projects">
<% @projects.each do |project| %>
<h2><%= link_to project.name, project %></h2>
<p><%= project.descript %></p>
<% end %>
</div>
</header>
