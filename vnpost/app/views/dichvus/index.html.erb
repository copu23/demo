
<head>
  <title>Dịch vụ</title>
</head>
<div class="row">
  <div class="col-md-8"><h1 class="text-center" style="margin: 0px; padding-top: 20px">Dịch vụ</h1></div>
  <div class="col-md-4" style="padding-top: 20px"><%= link_to "Thêm mới", new_admin_dichvu_path, class: "new" %></div>
</div>
<div>
<table id="mytable" class="table table-bordred table-striped">
  <thead>
    <tr>
      <th>Dịch vụ</th>
      <th>Mô tả </th>
      <th> Tỷ lệ gía trị</th>
      <th> </th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<% @dichvus.each do |dichvu| %>
<tr>
<td><%= link_to dichvu.tendv, dichvu %></td>
<td><%= dichvu.mota %></td>
<td><%= dichvu.tylegiatri %></td>
<td>
<%= link_to t('.show', :default => t("helpers.links.show")),
                      dichvu_path(dichvu), class: "show" %></td>
<td><%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_dichvu_path(dichvu), class: "edit" %></td>

<td>
<% admins_only do %>

<td><%= link_to "Xóa", admin_dichvu_path(dichvu),method: :delete,data: { confirm: "Bạn có chắc chắn muốn xóa không?" },class: "delete" %></td>
<% end %>
</td>
</tr>
<% end %>
</tbody>
</table>
</div>
