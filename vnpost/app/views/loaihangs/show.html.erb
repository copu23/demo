
<header>

<h1><%= @loaihang.tenlh %></h1>
<%= @loaihang.mota %>
<ul class="actions">
<% if policy(@loaihang).update? %>
<li><%= link_to "Edit", edit_loaihang_path(@loaihang),
class: "edit" %></li>
<% end %>
<% admins_only do %>
<li><%= link_to "Delete", admin_loaihang_path(@loaihang),method: :delete,data: { confirm: "Are you sure you want to delete this loaihang?" },class: "delete" %></li>
<% end %>
</ul>
</header>