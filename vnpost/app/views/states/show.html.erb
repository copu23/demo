<header>

<h1><%= @state.name %></h1>
<%= @state.color %>


<ul class="actions">
<li><%= link_to "Chỉnh sửa", edit_state_path(@state),
class: "edit" %></li>
<% admins_only do %>
<li><%= link_to "Xóa", admin_state_path(@state),method: :delete,data: { confirm: "Bạn có chắc chắn muốn xóa không?" },class: "delete" %></li>
<li><%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                states_path, class: "cancel" %></li>
<% end %>
</ul>
</header>
