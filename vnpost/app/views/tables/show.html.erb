
<header>

<h1><%= @table.name %></h1>
<%= @table.mota %>
<ul class="actions">
<li><%= link_to "Edit", edit_table_path(@table),
class: "edit" %></li>

<% admins_only do %>
<li><%= link_to "Delete", admin_table_path(@table),method: :delete,data: { confirm: "Are you sure you want to delete this table?" },class: "delete" %></li>
<% end %>
</ul>
</header>