<style>
ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}

li {
    display: inline;
}
</style>
<%= simple_form_for(@vung) do |f| %>
<%= f.input :tenvung,label: "Tên vùng" %>

<ul id="action">
<li><%= f.button :submit, class: "btn-primary" %></li>
<li><%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                vungs_path, class: "cancel" %></li>
</ul>
<% end %>
